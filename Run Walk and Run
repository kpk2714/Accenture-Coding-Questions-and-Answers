import java.util.Scanner;

public class getWays
{

	static int fibo(int n)
	{
		if(n==0)
			return 0;
		else if(n==1 || n==2)
			return 1;
		return fibo(n-1)+fibo(n-2);
	}

	static int get(int n)
	{
		int num = fibo(n+1);
		return num;
	}

	public static void main(String[] args)
	{
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		System.out.print(get(n));
	}
}
